{
{1032140,new Vector3("-488.9143,41.58625,-295.5215")},
{1032141,new Vector3("-499.321, 45.80337, -313.9849")},
{1032142,new Vector3("-486.7476, 43.74245, -304.1276")},

};
		await CommonTasks.StopAndDismount();
		
		var activeId = 0u;
		foreach(var id in positionDictionary.Keys)
		{
			var gameobj = GameObjectManager.GetObjectByNPCId(id);
			if (gameobj != null && gameobj.IsTargetable)
			{
				activeId = id;
				break;
			}
		}
		
		
		
		
		while (Core.Player.Location.Distance(positionDictionary[activeId]) > 3f)
        {
			await CommonTasks.MoveTo(positionDictionary[activeId]);
			await Coroutine.Yield();
        }
		
	  
        if (GameObjectManager.GetObjectByNPCId(activeId).IsVisible)  {  ChatManager.SendChat("/s snae ling");  }

        while (ConditionParser.GetQuestStep(69227) != 255)
        {
          if (Talk.DialogOpen)  {  Talk.Next();  }
          await Coroutine.Yield();
        }
      ]]>
