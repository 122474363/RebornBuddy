    <CodeChunk Name="Camp_Tranquil_Jump"><![CDATA[ 
        if(Core.Player.IsMounted)
        {
          ActionManager.Dismount();
          await Coroutine.Sleep(3000);
        }
        MovementManager.SetFacing(0.8310049f);
        MovementManager.MoveForwardStart();
